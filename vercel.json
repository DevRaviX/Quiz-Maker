{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "env": {
    "VITE_GOOGLE_API_KEY": "@vite_google_api_key"
  },
  "functions": {
    "server/proxy.mjs": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/server/proxy.mjs"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
